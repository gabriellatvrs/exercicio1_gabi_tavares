v[4]=v[1]*v[3];
RESULT(v[3])


EQUATION("P") // P = Preço no nível da Firma
v[0]=VL("X_Marketshare", 1);
v[1]=V("X_Marketshare");
		if(v[1]>=v[0]) 	
		v[2]=((v[1]-v[0])/v[0])+1; 
		else 
		v[2]=1;
v[3]=VL("P",1);
v[4]=v[2]*v[3];
RESULT(v[4])


EQUATION("QUAL") // QUAL = Qualidade no nível da Firma
